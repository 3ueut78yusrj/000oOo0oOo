function tentEdit(tent_id)
    if tent_id == nil then
        gg.alert("Something Went Error")
        return
    end
    local searchDone = false
    
    while not searchDone do
    gg.setVisible(false)
    local AddValuetent = 8192 + tent_id
    gg.setRanges(gg.REGION_CODE_APP)
    gg.searchNumber("-21,480W;536,888,835:3", gg.TYPE_DWORD)
    gg.refineNumber("-21,480", gg.TYPE_WORD)
    local mrr = gg.getResults(999)

    if #mrr > 0 then
        local mttt = {}
        mttt[1] = {}
        mttt[2] = {}
        mttt[1].address = mrr[1].address - 0x12
        mttt[1].flags = gg.TYPE_WORD
        mttt[1].value = -16640 -- NOP
        mttt[2].address = mrr[1].address - 0x10
        mttt[2].flags = gg.TYPE_WORD
        mttt[2].value = AddValuetent
        gg.setValues(mttt)
        gg.clearResults()
        gg.toast("✓ Tent ID: "..tent_id.." ✓")
        searchDone = true
    else
        gg.toast("✘No results found✘")
        end
    end
end

