function searchAndReplace()
gg.setRanges(gg.REGION_JAVA_HEAP)
local Typeprompt = gg.prompt({"Enter Your LoggerID:\nif you dont what is LoggerID, ask edittex:"}, {}, {"text"})

    -- Handle "Back" button or empty input
    if Typeprompt == nil or Typeprompt[1] == "" then
        gg.alert("You didnt Input something")
        main()
        return
    end
    
    if #Typeprompt[1] < 21 then
        gg.alert("Sorry, Your LoggerID is too short.")
        searchAndReplace()
        return
    end
    
    local searchStringUTF8 = ":" .. Typeprompt[1]
    local searchStringUTF16 = ";" .. Typeprompt[1]
    
    gg.searchNumber(searchStringUTF8, gg.TYPE_BYTE)
        gg.getResults(999)
        gg.editAll(":gid_a_obkbtbu87ou8jrg", gg.TYPE_BYTE)
        gg.clearResults()
    
    gg.searchNumber(searchStringUTF16, gg.TYPE_WORD)
        gg.getResults(999)
        gg.editAll(";gid_a_obkbtbu87ou8jrg", gg.TYPE_WORD)
        gg.clearResults()

    if gg.getResultsCount() == 0 then
        gg.alert("Successfully! 😇👍")
    main()
end
end

