function AS()
gg.clearResults()
gg.setRanges(gg.REGION_CODE_APP)
gg.searchNumber("9,500,880;4,454,544:5", gg.TYPE_DWORD, false, gg.SIGN_EQUAL, 0, -1, 0)
gg.refineNumber("9500880", gg.TYPE_DWORD, false, gg.SIGN_EQUAL, 0, -1, 1)
gg.setVisible(false)
if gg.getResultsCount() > 0 then
local addr = gg.getResults(1)[1].address
local values = {{
	address = addr - 0x4,
	flags = gg.TYPE_FLOAT,
}}
values = gg.getValues(values)
gg.clearResults()
local input = gg.prompt({"Set SPEED slime movement: 🟩Anti-Slime🤢\n---------------\n1 = Normal slime speed x0.3\n2 = x0.4 Speed\n3 = x0.5 Speed\n4 = x0.6 Speed\n---------------\n⚠️SERVER KICK LIMTIED SPEED⚠️:\n5 = x0.7 Speed\n6 = x0.8 Speed\n7 = x0.9 Speed\n8 = x1.0 Speed[1;8]"}, {1}, {'number'})
	if input == nil then return nil
	else
		local value = tonumber(input[1])
		if value == 1 then value = 0.3
		elseif value == 2 then value = 0.4
		elseif value == 3 then value = 0.5
		elseif value == 4 then value = 0.6
		elseif value == 5 then value = 0.7
		elseif value == 6 then value = 0.8
		elseif value == 7 then value = 0.9
        elseif value == 8 then value = 1
		end
		values[1].value = value
		gg.setValues(values)
		gg.toast("🟩Anti-Slime🤢 Speed: ".. value)
	    end
	else
	    gg.alert("🟩Anti-Slime🤢 Not working, Sorry!")
	end
end
