function smoke()
    smokes = gg.choice({
        "1 - 99999.0s (Infinite)",
        "2 - 3.0s (Very Slow)",
        "3 - 2.0s (Slow)",
        "4 - 1.5s (75% Slow)",
        "5 - 1.0s (Slightly Slow)",
        "6 - 0.5s (Normal)"
    }, nil, "🚬 Choose a Time for Smoke 🚬")

    if smokes == 1 then INF() end
    if smokes == 2 then VERYSLOW() end
    if smokes == 3 then SLOW() end
    if smokes == 4 then SLOW75() end
    if smokes == 5 then SLOWL() end
    if smokes == 6 then NORMAL() end
end

local smokeInf = 'h 4F F6 FF 70 07 E0' -- Infinite
local smokeVerySlow = 'h 40 F6 B9 30 07 E0' -- Very Slow
local smokeSlow = 'h 40 F2 D1 70 07 E0' -- Slow
local smoke75 = 'h 40 F2 DD 50 07 E0' -- 75% Slow
local smokeLow = 'h 40 F2 E9 30 07 E0' -- Slightly Slow
local smokeNormal = 'h 4F F4 FA 70 07 E0' -- Normal

function applyAllSmoke(newHex, toastMessage)
    local allSmokes = {smokeInf, smokeVerySlow, smokeSlow, smoke75, smokeLow, smokeNormal}
    gg.setRanges(gg.REGION_CODE_APP)
    for _, originalHex in ipairs(allSmokes) do
        gg.searchNumber(originalHex, gg.TYPE_BYTE)
        gg.getResults(99)
        gg.editAll(newHex, gg.TYPE_BYTE)
        gg.clearResults()
    end
    gg.toast(toastMessage)
end

function INF()
    applyAllSmoke(smokeInf, "✅ Smoke: 99999.0s ✅")
end

function VERYSLOW()
    applyAllSmoke(smokeVerySlow, "⚡ Smoke: 3.0s⚡")
end

function SLOW()
    applyAllSmoke(smokeSlow, "🐌 Smoke: 2.0s 🐌")
end

function SLOW75()
    applyAllSmoke(smoke75, "🏃 Smoke : 1.5s 🏃")
end

function SLOWL()
    applyAllSmoke(smokeLow, "🛌 Smoke: 1.0s 🛌")
end

function NORMAL()
    applyAllSmoke(smokeNormal, "Smoke: Normal")
end

