    if LT == 1 then Longname() end gg.setVisible(false)
    if MI == 1 then Longmessage() end gg.setVisible(false)
    if CU == 1 then cardUpgrade() end gg.setVisible(false)
    if FSS == 1 then FS() end gg.setVisible(false)
    if BO == 1 then ASA() end gg.setVisible(false)
    if FB == 1 then BSB() end gg.setVisible(false)
    if IB == 1 then CSC() end gg.setVisible(false)
    if AC == 1 then XHX() end gg.setVisible(false)
    if ASS == 1 then AS() end gg.setVisible(false)
    if SR == 1 then headstun() end gg.setVisible(false)
    if UD == 1 then unitem() end gg.setVisible(false)
    if CP == 1 then powers() end gg.setVisible(false)
    if MF == 1 then mines() end gg.setVisible(false)
    if BF == 1 then fences() end gg.setVisible(false)
    if TT == 1 then taunt() end gg.setVisible(false)
    if FT == 1 then tfast() end gg.setVisible(false)
    if IG == 1 then greet() end gg.setVisible(false)
    if SP == 1 then showMC() end gg.setVisible(false)
    if XR1 == 1 then xray1() end gg.setVisible(false)
