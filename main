--[[function updateScript()
  local currentVersion = "1.3"
  local versiongame = gg.makeRequest('https://pastebin.com/raw/x5xy91ee').content
  local serverVersionContent = gg.makeRequest("https://pastebin.com/raw/KAbSNTYe").content
  local serverVersion = tonumber(serverVersionContent)
 
  if tonumber(currentVersion) < serverVersion then
    local choice = gg.choice({"📃 Download", "✖️ Cancel"}, nil, "⚙️ A new version "..serverVersion)
 
    if choice == 1 then
      local url = 'https://pastebin.com/raw/mtQ7J8es'
      local response = gg.makeRequest(url)
      if not response.content then
        gg.alert('❌ An error occurred during loading')
        os.exit()
      end
 
      local scriptContent = response.content
 
      local dir = gg.EXT_STORAGE .. '/'..versiongame..'_BomberFriends_v'..serverVersionContent..'_.lua'
      local file = io.open(dir, "w+")
      file:write(scriptContent)
      file:close()
 
      gg.alert("⚙️ Update completed!\n\nℹ️Name: "..versiongame.."_BomberFriends_v"..serverVersionContent.."_.lua\n👉Memory: /storage/emulated/0/\nYou Can Delete Old")
 
      local updatedScript = load(scriptContent)
      updatedScript()
      
    elseif choice == 2 then
      gg.alert("You are now use old version")
      main()
    else
      gg.alert("⚠️ Please make a valid choice")
      updateScript()
    end
  else
    gg.alert("Script version : "..currentVersion.."\nNo Need Update")
    os.remove("5.11_BomberFriends_v1.2_.lua")
    main()
  end
end]]

