local localConfigFile = gg.EXT_STORAGE .. "/GuardianPassConfig.DoNotDelete"

local function loadConfigFile()
    local data = {}
    local dataFunc, err = loadfile(localConfigFile)
    if dataFunc then
        local success, loadedData = pcall(dataFunc)
        if success and type(loadedData) == "table" then
            data = loadedData
        else
            print("Error loading config file:", loadedData)
        end
    else
        print("Error loading config file:", err)
    end
    return data
end

local function saveConfigFile(data)
    local success = gg.saveVariable(data, localConfigFile)
    if not success then
    end
end

local function Login()
    gg.setVisible(false)
    local password = "Yosef_is_great"
    local gg_EXT_STORAGE = gg.EXT_STORAGE or "/sdcard"
    local configFilePath = gg_EXT_STORAGE .. "/config_v1.lua"
    local userInput = loadConfigFile()
    local CheckConfig = ""
    if fileExists(configFilePath) then
        CheckConfig = "✅Found✅"
    else
        CheckConfig = "❌Not Found❌"
    end
    while true do
        local result = gg.prompt({"Type password", "load config file\n" .. CheckConfig}, userInput, {"text", "checkbox"})
        if result then
            userInput = result
            saveConfigFile(userInput)
            if userInput[1] ~= password then
                local failedLoginHandler = gg.alert("!!✘ Wrong password ✘!!", "Try Again", nil, "Exit")
                if failedLoginHandler == 1 then
                else
                    print("👏 Try again after you get the password 👏")
                    os.exit()
                end
            else
                gg.setVisible(false)
                gg.clearResults()
                gg.clearList()
                print("❤️Thanks for Use my Script👍")
                gg.sleep(200)
                gg.setVisible(true)
                break
            end
        else
            return Login()
        end
    end
    return userInput
end

