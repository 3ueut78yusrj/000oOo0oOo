function alledit(textureID)
    local AddValueAll = 8192 + textureID

    gg.setRanges(gg.REGION_CODE_APP)
    gg.searchNumber("18,025W;12,428W:3", gg.TYPE_WORD)
    gg.refineNumber("12428", gg.TYPE_WORD)
    local r = gg.getResults(1)
    gg.clearResults()

    if #r > 0 then
        local mtt = {}
        mtt[1] = {}
        mtt[2] = {}
        mtt[1].address = r[1].address + 0x2C
        mtt[1].flags = gg.TYPE_WORD
        mtt[1].value = AddValueAll
        mtt[2].address = r[1].address + 0x2E
        mtt[2].flags = gg.TYPE_WORD
        mtt[2].value = 18288
        gg.setValues(mtt)
        gg.clearResults()
        gg.toast("✓Texture ID: "..textureID.." ✓")
        main()
    else
        gg.toast("✘No results found✘")
    end
end

function tilecolor_menu1()
    local localConfigFilee = gg.EXT_STORAGE .. "/GuardianTileConfig.1DoNotDelete"

    local function loadConfigFilee()
        local dataa = {}
        local dataFuncc = loadfile(localConfigFilee)
        if dataFuncc then
            dataa[1] = dataFuncc()
            dataa[2] = nil
            dataa[3] = nil
        end
        return dataa
    end

    local function saveConfigFilee(dataa)
        gg.saveVariable(dataa, localConfigFilee)
    end

    local dataa = loadConfigFilee()

    local tilemodel = gg.prompt(
        {"Input your Favorite texture 🆔       |       Return/Normal = 0 ;[0;47]", "[ Back = Cancel ]\nno active anything"}, 
        dataa,
        {[1] = 'number', [2] = 'checkbox'}
    )

    if tilemodel == nil then 
        tilecolor_main()
        return
    end
    
    saveConfigFilee(tilemodel)

    if tilemodel[2] then 
        tilecolor_main()
        return
    end

    local textureID = tonumber(tilemodel[1])

    if textureID == 0 then
        AllreturnValue()
        return
    elseif textureID >= 1 then
        alledit(textureID)
        return
    end
end

