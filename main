local function fileExists(filename)
    local file = io.open(filename, "r")
    if file then
        file:close()
        return true
    else
        return false
    end
end

local function writeConfigFile(filename, content)
    local file_path = gg_EXT_STORAGE .. "/" .. filename

    if fileExists(file_path) then
        local confirm = gg.alert("⚠️There is already a config file⚠️, at:\n\n" .. file_path .. "\n\n?Do you want to Create a New One?", "Yes", "No")
        if confirm == 2 then
            return nil
        end
    end

    local file = io.open(file_path, "w")
    if file then
        file:write(content)
        file:close()
        print("Config file created Successfully! at:\n\n" .. file_path)
        return file_path
    else
        print("Error creating configuration file.")
        return nil
    end
end

local config_path = writeConfigFile("config_v1.lua", config)

if config_path then
    gg.alert("Config file created at:\n" .. config_path)
end
end

