function tilecolor_menu2()
local localConfigFileee = gg.EXT_STORAGE .. "/GuardianTileConfig.2DoNotDelete"

    local function loadConfigFileee()
        local dataaa = {}
        local dataFunccc = loadfile(localConfigFileee)
        if dataFunccc then
            dataaa = dataFunccc()
        end
        return dataaa
    end

    local function saveConfigFileee(dataaa)
        gg.saveVariable(dataaa, localConfigFileee)
    end

    local dataaa = loadConfigFileee()
    
    local hhh = gg.prompt({
        "Stone 🆔       |       ;[1;47]", --1 addspritebatchfile / addObjectSprite ^ gettiletype
        "Tent 🆔    |     ;[1;47] ", --2 addObjectSprite
        "Floor 🆔    |    ;[1;47] ", --3 initTileSprites
        "Background 🆔    |    ;[0;47]", --4
        "✨Background Magick✨    |    Return/Normal = 0\n1 = 🌠Black Magick🌠;[0;1]", --5
        "[ Back = Cancel ]\nno active anything" --6
    }, dataaa, {[1] = 'number', [2] = 'number', [3] = 'number', [4] = 'number', [5] = 'number', [6] = 'checkbox'})

    if hhh == nil then
        tilecolor_main()
        return
    end

    saveConfigFileee(hhh)

    if hhh[6] then
        tilecolor_main()
        return
    end
    
    local stone_id = tonumber(hhh[1])
    local tent_id = tonumber(hhh[2])
    local floor_id = tonumber(hhh[3])
    local background_id = tonumber(hhh[4])
    local background_magic_id = tonumber(hhh[5])

    if stone_id and stone_id > 0 then
        stoneEdit(stone_id)
    else
        stoneNormal()
    end

    if floor_id and floor_id > 0 then
        floorEdit(floor_id)
    else
        floorNormal()
    end

    if tent_id and tent_id > 0 then
        tentEdit(tent_id)
    else
        tentNormal()
    end

    if background_id and background_id > 0 then
        backgEdit(background_id)
    else
        backgNormal()
    end

    if background_magic_id == 0 then
        backmNormal()
    elseif background_magic_id == 1 then
        backmBlack()
    end
end

function backmNormal()
    local searchDone = false
    
    while not searchDone do
        gg.setVisible(false)
        gg.searchNumber("1,177,559,316;1,200,637,481:5", gg.TYPE_DWORD)
        gg.refineNumber("1,200,637,481", gg.TYPE_DWORD)
        local mrrbr = gg.getResults(999)
        
        if #mrrbr > 0 then
            local mtttfbr = {}
            mtttfbr[1] = {}
            mtttfbr[1].address = mrrbr[1].address + 0x3A
            mtttfbr[1].flags = gg.TYPE_WORD
            mtttfbr[1].value = -19688
            gg.setValues(mtttfbr)
            gg.clearResults()
            gg.toast("✓Background-Magic : False✓")
            searchDone = true
            main()
        else
            gg.toast("✘No results found✘")
            gg.sleep(1000)
        end
    end
end

