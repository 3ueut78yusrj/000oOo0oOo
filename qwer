function I()
gg.searchNumber("1;8F;2;8F;2;8F;3;8F;3;10F;4;10F;4;10F;5;10F;5;12F;1~20::", gg.TYPE_DWORD, false, gg.SIGN_EQUAL, 0, -1, 0)
