gg.clearResults()
gg.toast("ðŸ’¼Items X100â™¾ï¸ | OFF")
end

local function toggleScript()
  scriptAtivado = not scriptAtivado
  if scriptAtivado then
    modifyValues()
  else
    restoreValues()
  end
end

-- Adicionando um loop para manter o script em execuÃ§Ã£o
