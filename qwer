end

function J()
gg.searchNumber("2.0;2.3;2.6;2.9;3.2;3.5;3.8;4.1;4.5", gg.TYPE_FLOAT, false, gg.SIGN_EQUAL, 0, -1, 0)
gg.processResume()
gg.getResults(1000, nil, nil, nil, nil, nil, nil, nil, nil)
gg.editAll("9999" , gg.TYPE_FLOAT)
gg.toast("ðŸŒInfinityðŸŒ")
